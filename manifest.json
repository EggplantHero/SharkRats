{
  "name": "sharkrats",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SharkRats"
  },
  "icons": {
    "16": "shark-rats-logo.png",
    "32": "shark-rats-logo.png",
    "48": "shark-rats-logo.png",
    "128": "shark-rats-logo.png"
  },
  "permissions": ["tabs"]
}
